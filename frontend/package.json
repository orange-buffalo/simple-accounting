{
  "name": "frontend",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "bun build-config/post-install/post-install.js && bun graphql-codegen",
    "dev": "bun run --bun vite",
    "build": "bun run --bun typecheck && bun run --bun vite build",
    "test:unit": "vitest --run",
    "test:unit:watch": "vitest",
    "typecheck": "bun run --bun vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "typecheck-storybook": "bun run --bun vue-tsc --noEmit -p tsconfig.storybook-config.json --composite false",
    "lint": "bun run --bun eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "storybook": "storybook dev -p 6006 -c build-config/storybook",
    "build-storybook": "bun run typecheck-storybook && storybook build -c build-config/storybook -o build/storybook",
    "codegen": "graphql-codegen"
  },
  "dependencies": {
    "@element-plus/icons-vue": "2.3.2",
    "@messageformat/core": "3.4.0",
    "@messageformat/parser": "5.1.1",
    "@popperjs/core": "^2.11.8",
    "@urql/vue": "^2.0.0",
    "copy-to-clipboard": "3.3.3",
    "dompurify": "3.2.6",
    "dropzone": "6.0.0-beta.2",
    "element-plus": "2.11.2",
    "event-source-polyfill": "1.0.31",
    "globalize": "1.7.0",
    "graphql": "^16.11.0",
    "imask": "6.6.3",
    "jwt-decode": "4.0.0",
    "lodash": "4.17.21",
    "marked": "16.3.0",
    "qs": "6.14.0",
    "vue": "3.5.21",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/client-preset": "^5.0.0",
    "@rushstack/eslint-patch": "1.12.0",
    "@sinonjs/fake-timers": "14.0.0",
    "@storybook/addon-actions": "8.1.11",
    "@storybook/addons": "7.6.17",
    "@storybook/builder-vite": "9.1.5",
    "@storybook/manager-api": "8.0.6",
    "@storybook/theming": "8.1.11",
    "@storybook/vue3": "8.1.11",
    "@storybook/vue3-vite": "7.6.17",
    "@types/cldrjs": "0.4.28",
    "@types/dompurify": "3.2.0",
    "@types/dropzone": "5.7.9",
    "@types/event-source-polyfill": "1.0.5",
    "@types/flat": "5.0.5",
    "@types/globalize": "1.5.5",
    "@types/js-yaml": "4.0.9",
    "@types/jsdom": "21.1.7",
    "@types/lodash": "4.17.20",
    "@types/marked": "6.0.0",
    "@types/node": "24.4.0",
    "@types/qs": "6.14.0",
    "@types/sinonjs__fake-timers": "8.1.5",
    "@vitejs/plugin-vue": "6.0.1",
    "@vue/eslint-config-airbnb": "8.0.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "@vue/test-utils": "2.4.6",
    "cldr-data": "36.0.4",
    "cldrjs": "0.5.5",
    "eslint": "8.57.1",
    "eslint-plugin-storybook": "0.12.0",
    "eslint-plugin-vue": "9.33.0",
    "fetch-mock": "12.5.4",
    "flat": "6.0.1",
    "js-yaml": "4.1.0",
    "jsdom": "26.1.0",
    "rollup-plugin-visualizer": "6.0.3",
    "sass": "1.92.1",
    "storybook": "7.6.17",
    "typescript": "5.9.2",
    "vite": "7.1.5",
    "vite-svg-loader": "5.1.0",
    "vitest": "3.2.4",
    "vue-tsc": "3.0.7",
    "whatwg-fetch": "3.6.20"
  }
}
