{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "node build-config/npm-post-install/post-install.js",
    "dev": "vite",
    "build": "npm run typecheck && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --run",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "storybook": "start-storybook -p 6006 -c build-config/storybook",
    "build-storybook": "build-storybook -c build-config/storybook -o build/storybook",
    "build-svgs": "svgo -f src/icons/svg -o src/icons/svg --config build-config/svgo.config.js"
  },
  "dependencies": {
    "@messageformat/core": "^3.0.1",
    "element-plus": "^2.2.8",
    "flat": "^5.0.2",
    "globalize": "^1.7.0",
    "jwt-decode": "^3.1.2",
    "qs": "^6.11.0",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@mdx-js/react": "^2.1.2",
    "@rushstack/eslint-patch": "^1.1.4",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addons": "^6.5.9",
    "@storybook/builder-vite": "^0.1.38",
    "@storybook/theming": "^6.5.9",
    "@storybook/vue3": "^6.5.9",
    "@types/flat": "^5.0.2",
    "@types/globalize": "^1.5.2",
    "@types/js-yaml": "^4.0.5",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.27",
    "@types/qs": "^6.9.7",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "babel-loader": "^8.2.5",
    "cldr-data": "^36.0.1",
    "cldrjs": "^0.5.5",
    "eslint": "^8.18.0",
    "eslint-plugin-storybook": "^0.5.13",
    "eslint-plugin-vue": "^9.1.1",
    "fetch-mock": "^9.11.0",
    "js-yaml": "^4.1.0",
    "jsdom": "^20.0.0",
    "sass": "^1.53.0",
    "typescript": "~4.7.4",
    "unplugin-auto-import": "^0.9.2",
    "unplugin-vue-components": "^0.21.0",
    "vite": "^2.9.13",
    "vite-svg-loader": "^3.4.0",
    "vitest": "^0.16.0",
    "vue-tsc": "^0.38.5"
  }
}
